<script>
	$(document).ready(function() {
		$('#loading').show(0);
		$('#page-wrapper').hide(0);
		$("#navtoggle").on("click", "a.untoggled", function(e) {
			e.preventDefault()
			$(this).removeClass("untoggled").addClass("toggled").html('<i class="icon-minus"></i>');
			$("#page-wrapper").css({
				'margin-left': '220px'
			});
			$("#access").css({
				'margin-left': '0px'
			});
		});
		$("#navtoggle").on("click", "a.toggled", function(e) {
			e.preventDefault()
			$(this).removeClass("toggled").addClass("untoggled").html('<i class="icon-plus"></i>');
			$("#page-wrapper").css({
				'margin-left': '0px'
			});
			$("#access").css({
				'margin-left': '-220px'
			});
		});
	});
	
	$(window).load(function() {
		$('#page-wrapper').show(0);
		$('#loading').css({'opacity':'0'}).delay(500).hide(0);
	});
</script>

